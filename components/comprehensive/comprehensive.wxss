.none{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 700;
  color: #999;
  margin-top: 80rpx;
}
.single{
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content: space-between;
  width: 95%;
  padding: 20rpx 2.5% 0 2.5%;
}
.single .font{
  font-size: 28rpx;
  font-weight: 700;
}
.single .right{
  height: 40rpx;
  width: 150rpx;
  border: 2rpx solid lightgray;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.single .right image{
  width: 30rpx;
  height: 30rpx;
}
.single .right .font{
  font-size: 18rpx;
  margin-left: 6rpx;
  color: #333;
}
.songslist{
  width: 95%;
  height: 80rpx;
  display: flex;
  align-items: center;
  padding: 20rpx 2.5%;
}
.songslist .song{
  width: 85%;
  height: 80rpx;
}
.songslist .song .songname{
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.songslist .song .name{
  font-size: 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #999;
}
.songslist .play{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.songslist .menu{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.moresong{
  font-size: 20rpx;
  color: #999;
  text-align: center;
  margin: 30rpx 0 30rpx 0;
}
.songsheet{
  font-size: 28rpx;
  font-weight: 700;
  width: 95%;
  height: 40rpx;
  padding: 30rpx 2.5%;
}
.playLists{
  display: flex;
  width: 95%;
  height: 160rpx;
  align-items: center;
  padding: 20rpx 2.5%;
}
.playLists image{
  height: 160rpx;
  width: 160rpx;
  border-radius: 10rpx;
}
.playLists .right{
  margin-left: 20rpx;
  width: 75%;
}
.playLists .right .name{
  font-size: 24rpx;
  line-height: 30rpx;
}
.playLists .right .count{
  font-size: 20rpx;
  color: #999;
}
.video{
  display: flex;
  width: 95%;
  height: 140rpx;
  align-items: center;
  padding: 20rpx 2.5%;
}
.video .left{
  width: 240rpx;
  height: 140rpx;
  position: relative;
}
.video .left image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.video .left .playtime{
  position: absolute;
  right: 10rpx;
  top: 0;
  height: 30rpx;
  display: flex;
  align-items: center;
}
.video .left .playtime image{
  width: 20rpx;
  height: 20rpx;
}
.video .left .playtime .time{
  font-size: 16rpx;
  color: white;
}
.video .right{
  margin-left: 20rpx;
  width: 60%;
}
.video .right .name{
  font-size: 24rpx;
  line-height: 30rpx;
}
.video .right .count{
  font-size: 20rpx;
  color: #999;
}
.querys{
  display: flex;
  flex-wrap: wrap;
}
.keyword{
  font-size: 24rpx;
  padding: 8rpx 20rpx;
  margin: 0 10rpx 30rpx 10rpx;
  background: #f2f2f2;
  border-radius: 20rpx;
}
.mlog{
  display: flex;
  width: 95%;
  justify-content: space-between;
  padding: 0 2.5% 40rpx 2.5%;
}
.mlog .box{
  width: 48%;
  height: 460rpx;
  border-radius: 20rpx;
  border: 2rpx solid #f2f2f2;
}
.mlog .box image{
  width: 100%;
  height: 330rpx;
  border-radius: 20rpx 20rpx 0 0;
}
.mlog .box .text{
  width: 90%;
  padding: 10rpx 5% 0 5%;
  line-height: 30rpx;
  font-size: 20rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.mlog .box .bottom{
  display: flex;
  height: 50rpx;
  align-items: center;
  width: 90%;
  padding: 0 5%;
  justify-content: space-between;
}
.mlog .box .bottom .left{
  display: flex;
}
.mlog .box .bottom image{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
}
.mlog .box .bottom .nickname{
  font-size: 18rpx;
  color: #333;
  margin-left: 0;
}
.mlog .box .bottom .likedCount{
  font-size: 18rpx;
  color: #333;
  margin-right: 0
}
.talks{
  width: 95%;
  height: 160rpx;
  display: flex;
  align-items: center;
  padding: 0 2.5% 40rpx 2.5%;
}
.talks .left{
  height: 160rpx;
  width: 160rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.talks .left image{
  height: 160rpx;
  width: 320rpx;
  border-radius: 20rpx;
}
.talks .right{
  margin-left: 20rpx;
  width: 60%;
}
.talks .right .talkName{
  font-size: 24rpx;
  line-height: 30rpx;
  padding: 20rpx 0;
}
.talks .right .follows{
  font-size: 20rpx;
  color: #999;
}
.talks{
  display: flex;
  height: 160rpx;
  align-items: center;
  width: 95%;
  padding: 0 2.5% 40rpx 2.5%;
}
.talks image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.talks .right{
  font-size: 24rpx;
}
.songsheets{
  display: flex;
  height: 160rpx;
  align-items: center;
  width: 95%;
  padding: 0 2.5% 40rpx 2.5%;
}
.songsheets image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
}
.songsheets .right{
  margin-left: 20rpx;
  width: 60%;
}
.songsheets .right .name{
  font-size: 24rpx;
  line-height: 30rpx;
  padding: 20rpx 0;
}
.songsheets .right .artist{
  font-size: 20rpx;
  color: #999;
}
.user{
  display: flex;
  height: 160rpx;
  align-items: center;
  width: 95%;
  padding: 0 2.5% 40rpx 2.5%;
}
.user image{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
}
.user .right{
  margin-left: 20rpx;
  width: 60%;
}
.user .right .name{
  font-size: 24rpx;
  line-height: 30rpx;
  padding: 20rpx 0;
}

